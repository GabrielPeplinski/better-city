    "android": {
      "versionCode": 1,
      "package": "com.gabrielpeplinski.bettercitypublish",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/better-city-logo.png",
        "backgroundColor": "#082942"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f3441b48-551d-477f-bdcf-b7aa425389ee"
      }
    }
  }
}
