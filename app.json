{
  "expo": {
    "name": "Better City",
    "slug": "better-city-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/better-city-icon.jpg",
    "userInterfaceStyle": "light",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/better-city-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#082942"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
